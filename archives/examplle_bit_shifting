#include <stdio.h>
#include "libft.h"
int main(void)
{
char c = 'A';
char carr[30] = {'H', 'e', 'l', 'l', 'o'};

	printf("\nMethod 2\n");
	printf("Original: %s\n", carr);
	*(unsigned long int*)(&carr) >>= 8;
	carr[4] = c;
	printf("Shifted:  %s\n", carr);


}
